{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "removeComments": true,
    "checkJs": true,
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "incremental": true
  }
}
